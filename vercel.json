{
    "version": 2,
    "rewrites": [
        { "source": "/fonts/:path*", "destination": "/fonts/:path*" },
        { "source": "/imgs/:path*", "destination": "/imgs/:path*" },
        { "source": "/scripts/:path*", "destination": "/scripts/:path*" },
        { "source": "/views/:path*", "destination": "/views/:path*" }
    ]
}
